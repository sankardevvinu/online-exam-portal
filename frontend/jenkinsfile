pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                sh 'rm -rf online-exam-portal'
                sh 'git clone https://github.com/sankardevvinu/online-exam-portal.git'
            }
        }
        stage('Build') {
            steps {
                 sh 'docker build -t demo:1 .'
            }
        }
    }
}