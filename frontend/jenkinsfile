pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/sankardevvinu/online-exam-portal.git'
            }
        }
        stage('Build') {
            steps {
                sh 'docker build -t demo:1 "/var/lib/jenkins/workspace/nodejs project/online-exam-portal/frontend"'
            }
        }
    }
}
